generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ButtonClick {
  id        Int      @id @default(autoincrement())
  buttonText String
  ipAddress String
  timestamp DateTime @default(now())
} 