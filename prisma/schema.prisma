generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ButtonClick {
  id        Int      @id @default(autoincrement())
  buttonText String
  ipAddress String
  timestamp DateTime @default(now())
} 